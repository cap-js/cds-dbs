{
  "name": "@cap-js/postgres",
  "scripts": {
    "setup": "docker-compose -f pg-stack.yml up -d",
    "test": "npm run setup && jest --silent"
  },
  "dependencies": {
    "pg": "^8"
  },
  "peerDependencies": {
    "@sap/cds": ">=6.8.0"
  },
  "cds": {
    "requires": {
      "kinds": {
        "sql": {
          "[production]": {
            "kind": "postgres"
          }
        },
        "postgres": {
          "impl": "@cap-js/postgres",
          "dialect": "postgres"
        }
      },
      "db": "sql"
    }
  },
  "license": "SEE LICENSE"
}
